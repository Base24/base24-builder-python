rofi.color-active:  #3f4451, #61afef, #3f4451, #3f4451, #61afef
rofi.color-urgent:  #3f4451, #e06c75, #3f4451, #3f4451, #e06c75

! Set the desired separator style
rofi.separator-style: solid
